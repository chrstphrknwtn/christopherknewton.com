{
  "version": 2,
  "alias": ["christopherknewton.com"],
  "builds": [
    { "src": "/pages/*.js", "use": "@now/node" },
    { "src": "/static/**/*", "use": "@now/static" }
  ],
  "routes" : [
    { "handle": "filesystem" },
    { "src": "/", "dest": "/pages/index.js" },
    { "src": "/post", "dest": "/pages/post.js" },
    { "src": "/(?<slug>[^/]*)", "dest": "/pages/project.js?slug=$slug" }
  ]
}
